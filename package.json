  "main": "index.js",
  "scripts": {
    "start:prod": "env TZ=Asia/Tokyo ts-node -T src/server.ts",
    "start": "env TZ=Asia/Tokyo ts-node-dev --respawn --files src/server.ts",
    "db-start": "ts-node-dev --respawn src/index.ts",
    "ws-server:prod": "env TZ=Asia/Tokyo ts-node -T src/ws-server.ts",
    "ws-server": "env TZ=Asia/Tokyo ts-node-dev --respawn --files src/ws-server.ts",
    "build": "tsc",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "makemigrations": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "migrate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
    "migrate:revert": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@line/bot-sdk": "^7.3.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cheerio": "^1.0.0-rc.10",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "libxmljs2": "^0.26.7",
    "pg": "^8.6.0",
    "puppeteer": "^10.1.0",
    "redis": "^3.1.2",
    "textlint": "^12.0.2",
    "textlint-rule-preset-ja-technical-writing": "^6.0.0",
    "ts-node": "^10.1.0",
    "twitter-v2": "^1.1.0",
